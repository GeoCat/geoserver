 <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

  <bean id="enterpriseTheme" class="net.geocat.enterprise.geoserver.theme.GeoCatEnterpriseTheme">
      <!-- defines the resources folder -->
      <property name="scope" value="net.geocat.enterprise.geoserver.theme">
      <!-- identifies css file to include -->
      <property name="CSSFilename" value="css/geocat_enterprise.css"/>
  </bean>
 
</beans>